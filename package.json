{
  "name": "bunqldb",
  "version": "0.3.3",
  "description": "Bun 네이티브 SQL 클라이언트 기반 MySQL/PostgreSQL 호환 데이터베이스 모듈",
  "type": "module",
  "main": "./src/index.ts",
  "module": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "bun": "./src/index.ts",
      "import": "./src/index.ts"
    }
  },
  "files": [
    "src",
    "README.md"
  ],
  "engines": {
    "bun": ">=1.0.0"
  },
  "scripts": {
    "test": "bun test",
    "test:mysql": "bun --env-file=.env.test.mysql test",
    "test:postgres": "bun --env-file=.env.test.postgres test",
    "test:all": "bun run test:mysql && bun run test:postgres",
    "typecheck": "tsc --noEmit",
    "lint": "biome lint ./src ./test",
    "lint:fix": "biome lint --write ./src ./test",
    "format": "biome format ./src ./test",
    "format:fix": "biome format --write ./src ./test",
    "check": "biome check ./src ./test",
    "check:fix": "biome check --write ./src ./test",
    "validate": "bun run typecheck && bun run lint"
  },
  "keywords": [
    "bun",
    "sql",
    "database",
    "mysql",
    "postgresql",
    "orm",
    "transaction"
  ],
  "author": "mayajuni <mayajuni10@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mayajuni/bunqldb.git"
  },
  "bugs": {
    "url": "https://github.com/mayajuni/bunqldb/issues"
  },
  "homepage": "https://github.com/mayajuni/bunqldb#readme",
  "peerDependencies": {
    "bun-types": ">=1.0.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.11",
    "bun-types": "latest",
    "typescript": "^5.0.0"
  }
}